<form action="/dishes" method="post">
  
  <label for="">name</label>
  <input name="name" type="text">

  <label for="">image_url</label>
  <textarea name="image_url" id="" cols="30" rows="10"></textarea>

  <label for="">dish type</label>
  <select name="dish_type_id" id="">
    <% @dish_types.each do |dish_type| %>
    <option value="<%= dish_type.id %>"><%= dish_type.name %></option>
    <% end %>
  </select>

  <button>create</button>

</form>